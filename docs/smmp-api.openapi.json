  "paths": {
    "/upload-file": {
      "post": {
        "summary": "Upload file",
        "description": "Upload file",
        "operationId": "uploadFile",
        "requestBody": {
          "content": {
            "multipart/form-data": {
              "schema": {
                "type": "object",
                "properties": {
                  "file": {
                    "description": "archivo a subir",
                    "type": "string",
                    "format": "binary"
                  },
                  "metadata": {
                    "description": "los valores deben ser numeros o string",
                    "type": "string",
                    "example": "{\"subject\":\"test\"}"
                  }
                }
              }
            }
          }
        },
        "responses": {
          "200": {
            "description": "Upload file",
            "headers": {
              "Access-Control-Allow-Origin": {
                "schema": {
                  "type": "string",
                  "example": "*"
                }
              },
              "Connection": {
                "schema": {
                  "type": "string",
                  "example": "keep-alive"
                }
              },
              "Content-Length": {
                "schema": {
                  "type": "string",
                  "example": "271"
                }
              },
              "Date": {
                "schema": {
                  "type": "string",
                  "example": "Fri, 02 Dec 2022 07:24:26 GMT"
                }
              },
              "ETag": {
                "schema": {
                  "type": "string",
                  "example": "W/\"10f-8nGP4qbgEQNQxAvd14ZjKZGqlOI\""
                }
              },
              "Keep-Alive": {
                "schema": {
                  "type": "string",
                  "example": "timeout=5"
                }
              },
              "X-Powered-By": {
                "schema": {
                  "type": "string",
                  "example": "Express"
                }
              }
            },
            "content": {
              "application/json": {
                "schema": {
                  "type": "object",
                  "properties": {
                    "data": {
                      "type": "object",
                      "properties": {
                        "mimetype": {
                          "type": "string",
                          "example": "image/jpeg"
                        },
                        "name": {
                          "type": "string",
                          "example": "rintaro-1.jpg"
                        },
                        "size": {
                          "type": "number",
                          "example": 194547
                        }
                      }
                    },
                    "message": {
                      "type": "string",
                      "example": "File is uploaded"
                    },
                    "metadata": {
                      "type": "object",
                      "properties": {
                        "subject": {
                          "type": "string",
                          "example": "test"
                        }
                      }
                    },
                    "pinata": {
                      "type": "object",
                      "properties": {
                        "IpfsHash": {
                          "type": "string",
                          "example": "QmYSunLGem8Jqpr4ckDZjQhEywUya3T7Lxhpk1XbNMEg3j"
                        },
                        "PinSize": {
                          "type": "number",
                          "example": 194561
                        },
                        "Timestamp": {
                          "type": "string",
                          "example": "2022-12-02T07:24:26.275Z"
                        }
                      }
                    },
                    "status": {
                      "type": "boolean",
                      "example": true
                    }
                  }
                },
                "examples": {
                  "Upload file": {
                    "value": {
                      "data": {
                        "mimetype": "image/jpeg",
                        "name": "rintaro-1.jpg",
                        "size": 194547
                      },
                      "message": "File is uploaded",
                      "metadata": {
                        "subject": "test"
                      },
                      "pinata": {
                        "IpfsHash": "QmYSunLGem8Jqpr4ckDZjQhEywUya3T7Lxhpk1XbNMEg3j",
                        "PinSize": 194561,
                        "Timestamp": "2022-12-02T07:24:26.275Z"
                      },
                      "status": true
                    }
                  }
                }
              }
            }
          }
        }
      }
    },
